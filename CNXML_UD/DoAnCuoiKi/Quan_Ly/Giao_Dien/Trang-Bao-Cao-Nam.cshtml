<!DOCTYPE html>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1"><title>Trang Chính - Khách Hàng</title>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.1/css/materialize.min.css">
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<link rel="stylesheet" href="DD_Chinh.css" type="text/css">
<body>
    @using System.Xml;
    @{
        var Du_Lieu = XL_DU_LIEU.Khoi_dong_Du_lieu();
        var DS_ThongKe_Nam = Du_Lieu.DS_ThongKe_Nam(2017);
        var Chuoi_The_Hien_DS = new HtmlString("");
        if (DS_ThongKe_Nam != null)
        {
            var Chuoi_HTML_The_Hien_DS = Du_Lieu.Tao_Chuoi_HTML_Thong_Ke_Nam(DS_ThongKe_Nam);
            Chuoi_The_Hien_DS = new HtmlString(Chuoi_HTML_The_Hien_DS);
        }
    }
    <div class="container-fluid">
        <header class="green">
            <div class="container">
                <div class="row valign-wrapper white-text mb-0">
                    <div class="col s2"><a class="white-text" href="#"><h5>BDT-TT</h5></a></div>
                    <div class="col s9 center-align"><h4>Quản Lý</h4></div>
                </div>
            </div>
        </header>
        <nav>
            <div class="nav-wrapper">
                <div class="container">
                    <ul>
                        <li><a href="Trang-Chinh.cshtml">Đơn Giá</a></li>
                        <li><a href="Trang-Don-Hang.cshtml">Đơn Hàng</a></li>
                        <li><a href="Trang-Phan-Cong.cshtml">Phân Công</a></li>
                        <li><a href="Trang-Bao-Cao-Thang.cshtml">Thống Kê Tháng</a></li>
                        <li><a class="active" href="Trang-Bao-Cao-Nam.cshtml">Thống Kê Năm</a></li>
                    </ul>
                </div>
            </div>
        </nav>
        <div id="main_wrapper">
            <div class="container">
                <h4 class="center-align">Thống kê doanh thu Năm</h4>
                @Chuoi_The_Hien_DS                
            </div>
        </div>
        <footer class="green darken-3">
            <div class="center-align white-text">
                <h5>Đồ án cuối kì - Công nghệ XML và ứng dụng</h5><h6>
                    Nguyễn Đức Trung -
                    1581270
